    "os-name": "^6.1.0",
    "p-wait-for": "^5.0.2",
    "pdf-parse": "^1.1.1",
    "pkce-challenge": "^5.0.0",
    "playwright": "^1.55.0",
    "posthog-node": "^5.8.4",
