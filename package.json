  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@changesets/types": "^6.1.0",
    "@dotenvx/dotenvx": "^1.49.0",
    "@types/debug": "^4.1.12",
    "@types/diff-match-patch": "^1.0.36",
    "@types/express": "^5.0.3",
    "@types/glob": "^9.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.4.0",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vscode/test-electron": "^2.5.2",
    "c8": "^10.1.3",
    "chai": "^6.0.1",
    "cmpstr": "^3.0.3",
    "cross-os": "^1.5.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "execa": "^9.6.0",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "knip": "^5.63.1",
    "lint-staged": "^16.1.6",
    "mkdirp": "^3.0.1",
    "mocha": "^11.7.2",
    "module-alias": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "openai-api-mock": "^0.2.1",
    "prettier": "^3.6.2",
    "proxyquire": "^2.1.3",
    "rimraf": "^6.0.1",
    "sinon": "^21.0.0",
    "tcp-port-used": "^1.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "npx eslint --max-warnings=0 --fix --config eslint.config.mjs"
    ],
    "webview-ui/**/*.{ts,tsx}": [
      "npx eslint --max-warnings=0 --fix --config webview-ui/eslint.config.mjs"
    ]
  },
  "extensionSecretsPrefix": "thea_code_config_",
  "overrides": {
    "node-domexception": "^2.0.0",
    "jsdom": "^27.0.0",
    "formdata-node": "^6.0.3",
    "glob": "^11.0.3"
  }
}
