    "@types/clone-deep": "^4.0.4",
    "@types/pdf-parse": "^1.1.5",
    "@types/tmp": "^0.2.6",
    "@vscode/codicons": "^0.0.40",
    "axios": "1.12.2",
    "cheerio": "^1.1.2",
    "chokidar": "^4.0.3",
    "clone-deep": "^4.0.1",
    "data-urls": "^6.0.0",
    "default-shell": "^2.2.0",
    "delay": "^6.0.0",
    "diff": "^8.0.2",
