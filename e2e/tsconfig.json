{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "target": "ES2022",
    "lib": ["ES2022", "ESNext.Disposable", "DOM"],
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "outDir": "../out/src/e2e",
    "rootDir": "..",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "types": ["node"]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "src/suite.backup/**/*"
  ]
}