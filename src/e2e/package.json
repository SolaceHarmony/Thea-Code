{
	"name": "e2e",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "cd ../.. && npm run build",
		"compile": "rm -rf out && tsc -p .",
		"lint": "eslint src --ext ts",
		"check-types": "tsc --noEmit",
		"test": "cd ../.. && npm run build:extension && cd src/e2e && npm run compile && node out/suite/launch.js",
		"test:full": "cd ../.. && npm run build && cd src/e2e && npm run compile && node out/suite/launch.js",
		"ci": "npm run build && npm run test",
		"clean": "rimraf out .vscode-test ../../.vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.3.0",
		"@types/proxyquire": "^1.3.31",
		"@types/sinon": "^17.0.4",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"chai": "^6.0.1",
		"mocha": "^11.7.1",
		"proxyquire": "^2.1.3",
		"rimraf": "^6.0.1",
		"sinon": "^21.0.0",
		"typescript": "^5.9.2"
	},
	"overrides": {
		"glob": "^11.0.2"
	}
}
