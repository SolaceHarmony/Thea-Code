{
	"name": "e2e",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "cd ../.. && npm run build",
		"compile": "rm -rf out && tsc -p .",
		"lint": "eslint src --ext ts",
		"check-types": "tsc --noEmit",
		"test": "npm run build && npm run compile && node ../../out/src/e2e/runTest.js",
		"test:full": "npm run build && npm run compile && node ../../out/src/e2e/runTest.js",
		"ci": "npm run build && npm run test",
		"clean": "rimraf out .vscode-test ../.vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.3.0",
		"@types/sinon": "^17.0.4",
		"@vscode/test-electron": "^2.5.2",
		"mocha": "^11.7.1",
		"rimraf": "^6.0.1",
		"sinon": "^21.0.0",
		"typescript": "^5.9.2"
	},
	"overrides": {
		"glob": "^11.0.2"
	}
}
